# simple fibonacci calculator

n = read;
f1 = 0;
f2 = 1;
f3 = f1 + f2;

if n == 0 then
    output f1;
else
    if n == 1 then
        output f2;
    else
        while n > 2 do
            f1 = f2;
            f2 = f3;
            f3 = f1 + f2;
            n = n - 1;
        end

        output f3;
    end
end

